workspace(name = "diodb")

load("@bazel_tools//tools/build_defs/repo:git.bzl", "git_repository")

# Logging.
git_repository(
 name = "glog",
 remote = "git@github.com:google/glog.git",
 tag = "v0.4.0",
)

# Command line flags.
git_repository(
    name = "com_github_gflags_gflags",
    remote = "https://github.com/gflags/gflags.git",
    tag = "v2.2.2",
)

